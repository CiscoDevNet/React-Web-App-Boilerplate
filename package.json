{
  "name": "react-web-app-boilerplate",
  "version": "1.0.2",
  "description": "DevNet React Web App boilerplate",
  "main": "app/main.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack/common.js",
    "clean": "rimraf dist",
    "production": "npm run clean &&  cross-env NODE_ENV=production webpack --config config/webpack/production.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js"
  },
  "author": "Aikepaer Abuduweili <aaikepae@cisco.com>",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "cross-env": "5.1.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0-beta.12",
    "react-router-dom": "4.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.4.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "4.3.0",
    "css-loader": "0.28.10",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.13.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.15.0-rc.8",
    "jest": "22.1.0",
    "node-sass": "7.0.0",
    "open-browser-webpack-plugin": "0.0.5",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "strip-loader": "^0.1.2",
    "style-loader": "0.20.0",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.9.6"
  }
}
